{
  "name": "spothack",
  "version": "1.0.0",
  "description": "An project to download music/playlists from Spotify.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Darguima/SpotHack.git"
  },
  "homepage": "https://github.com/Darguima/SpotHack#readme",
  "author": "Darguima",
  "license": "MIT",
  "keywords": [
    "Spotify",
    "Youtube",
    "Download",
    "Music",
    "Track",
    "Playlist"
  ],
  "bugs": {
    "url": "https://github.com/Darguima/SpotHack/issues"
  },
  "main": "src/index.ts",
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --transpile-only --ignore-watch node_modules --no-notify src/index.ts",
    "devr": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules --no-notify src/index.ts",
    "knex:migrate": "knex migrate:latest --knexfile knexfile.ts",
    "knex:migrate:rollback": "knex migrate:rollback --knexfile knexfile.ts"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@types/node": "^14.11.2",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.10.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "ts-node-dev": "^1.0.0-pre.63",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@types/fluent-ffmpeg": "^2.1.16",
    "axios": "^0.20.0",
    "fluent-ffmpeg": "^2.1.2",
    "knex": "^0.21.6",
    "sqlite3": "^5.0.0",
    "ytdl-core": "^3.3.0"
  }
}
